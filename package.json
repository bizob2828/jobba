{
  "name": "jobba",
  "version": "0.1.0",
  "description": "Jobba the Hub",
  "main": "dist/lib/index.js",
  "scripts": {
    "build": "tsc && [ -d ./dist/bin ] && chmod -f +x ./dist/bin/* || exit 0",
    "clean": "rm -rf ./dist",
    "lint": "tslint -t stylish './src/**/*.ts' || exit 0",
    "lint:verbose": "tslint -t codeFrame './src/**/*.ts' || exit 0",
    "postinstall": "npm run build",
    "watch": "tsc -w"
  },
  "keywords": [
    "job",
    "hub"
  ],
  "author": "Ross Hadden",
  "license": "ISC",
  "dependencies": {
    "bull": "^3.4.2",
    "bull-arena": "git+https://github.com/ConnectAi/arena.git#dev",
    "joi": "^13.4.0",
    "koa": "^2.5.0",
    "koa-bodyparser": "^4.2.0",
    "koa-express": "^1.1.0",
    "koa-router": "^7.4.0",
    "koa-static": "^5.0.0",
    "lodash": "^4.17.10",
    "yawk": "git+https://github.com/rosshadden/yawk.git"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.21",
    "@types/bull": "^3.3.12",
    "@types/joi": "^13.3.0",
    "@types/koa": "^2.0.46",
    "@types/koa-bodyparser": "^5.0.1",
    "@types/koa-router": "^7.0.30",
    "@types/node": "^10.3.3",
    "tslint": "^5.9.1",
    "typescript": "^2.9.2"
  }
}
