{
  "name": "jobba",
  "version": "0.5.3",
  "description": "Jobba the Hub",
  "main": "dist/lib/index.js",
  "scripts": {
    "build": "tsc && [ -d ./dist/bin ] && chmod -f +x ./dist/bin/* || exit 0",
    "clean": "rm -rf ./dist",
    "lint": "tslint -t stylish './src/**/*.ts' || exit 0",
    "lint:verbose": "tslint -t codeFrame './src/**/*.ts' || exit 0",
    "postinstall": "npm run build",
    "watch": "tsc -w"
  },
  "keywords": [
    "job",
    "hub"
  ],
  "author": "Ross Hadden",
  "license": "ISC",
  "dependencies": {
    "bull": "^3.4.7",
    "bull-arena": "git+https://github.com/whitebox-co/arena.git#dev",
    "chalk": "^2.4.1",
    "joi": "^13.6.0",
    "koa": "^2.5.2",
    "koa-bodyparser": "^4.2.1",
    "koa-express": "^1.1.0",
    "koa-router": "^7.4.0",
    "koa-static": "^5.0.0",
    "lodash": "^4.17.10",
    "moment": "^2.22.2",
    "yawk": "git+https://github.com/rosshadden/yawk.git"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.23",
    "@types/bull": "^3.3.19",
    "@types/joi": "^13.4.2",
    "@types/koa": "^2.0.46",
    "@types/koa-bodyparser": "^5.0.1",
    "@types/koa-router": "^7.0.31",
    "@types/lodash": "^4.14.116",
    "@types/moment": "^2.13.0",
    "@types/node": "^10.7.1",
    "tslint": "^5.11.0",
    "typescript": "^3.0.1"
  }
}
