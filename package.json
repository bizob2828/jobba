{
  "name": "jobba",
  "version": "0.0.1",
  "description": "Jobba the Hub",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && [ -d ./dist/bin ] && chmod -f +x ./dist/bin/* || exit 0",
    "clean": "rm -rf ./dist",
    "lint": "tslint -t stylish './src/**/*.ts' || exit 0",
    "lint:verbose": "tslint -t codeFrame './src/**/*.ts' || exit 0",
    "watch": "tsc -w"
  },
  "keywords": [
    "job",
    "hub"
  ],
  "author": "Ross Hadden",
  "license": "ISC",
  "dependencies": {
    "bull": "^3.4.2",
    "koa": "^2.5.0",
    "koa-bodyparser": "^4.2.0",
    "koa-router": "^7.4.0"
  },
  "devDependencies": {
    "@types/bull": "^3.3.12",
    "@types/node": "^10.3.3",
    "nodemon": "^1.17.3",
    "tslint": "^5.9.1",
    "typescript": "^2.9.2"
  }
}
